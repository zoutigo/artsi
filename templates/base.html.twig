<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>

		<link
		rel="shortcut icon" type="image/x-icon" href="assets/template/assets/images/favicon.svg"/>
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<body>
		<!-- Start Header Area -->
		<x-search></x-search>

		{% include "base/header.html.twig" %}
		{% block body %}{% endblock %}
		{% include "base/footer.html.twig" %}

	</body>
</html>
